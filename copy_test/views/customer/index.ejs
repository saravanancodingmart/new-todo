<div class="container">
 	<h3> Users </h3>
 	<table class="table">
 		<tr>
 			<th>ID</th>
 			<th>Name</th>
 			<th>Email</th>
 			<th>Role</th>
 			<th></th>
 			<th></th>
			<th></th>
 		</tr>
 		<%_.each(customers,function(customer){%>
 		<tr data-id="<%=customer.id%>" data-mode="customer">
 		<% if( session.Customer.id!=customer.id){%>
 			<td> <%= customer.id %> </td>
 			<td> <%= customer.name %> </td>
 			<td> <%= customer.email %> </td>
 			<td> <%= customer.role %> </td>
 			<td> <a href="/customer/show/<%=customer.id%>" class="btn btn-sm btn-primary">SHOW</a> </td>
 			<td> <a href="/customer/edit/<%=customer.id%>" class="btn btn-sm btn-warning">EDIT</a></td>
 			
 			<td> <form action="/customer/destroy/<%= customer.id%>" method="POST">
 				<input type="hidden" name="_method" value="delete"/>
 				<input type="submit" class="btn btn-sm btn-danger" value="Delete"/>
 				<input type="hidden" name="_csrf" value="<%=_csrf%>"/>
 				</form> </td>
 		</tr>
 		<%}%>
 		<% }) %>
 	</table>
</div>



 